{
  "expo": {
    "name": "FyreTask",
    "slug": "FyreTask",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/fyreicon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/fyreicon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.k1gw4.FyreTask"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "assets/images/fyreicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/fyreicon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b9e7ed2d-214a-496f-a6cd-9af471be1563"
      }
    },
    "owner": "k1gw4"
  }
}
